{
  "name": "histograph-data",
  "version": "0.6.0",
  "description": "Data download and conversion tool for Histograph",
  "main": "index.js",
  "devDependencies": {
    "jscs": "^1.10.0",
    "jshint": "^2.6.0",
    "tape": "^3.4.0"
  },
  "scripts": {
    "test": "jshint *.js && jscs *.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/histograph/data.git"
  },
  "author": "Waag Society",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.2.11",
    "chalk": "^1.1.1",
    "geojsonhint": "^1.1.0",
    "highland": "^2.5.1",
    "histograph-config": "histograph/config",
    "histograph-schemas": "histograph/schemas",
    "is-my-json-valid": "^2.12.3",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1"
  },
  "jshintConfig": {
    "node": true,
    "globalstrict": false,
    "undef": true,
    "unused": true,
    "noarg": true
  },
  "jscsConfig": {
    "preset": "airbnb",
    "requireCamelCaseOrUpperCaseIdentifiers": null,
    "disallowMultipleVarDecl": true,
    "requireMultipleVarDecl": null
  }
}
